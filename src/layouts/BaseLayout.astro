---
import '../styles/global.css';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { BUSINESS, CITIES } from '../data/constants';

const {
  title = 'The KPS Group | Modern Business Infrastructure Engine | Austin TX',
  description = BUSINESS.blurb,
  canonical,
  noindex = false,
  ogImage = '/img/branding/og-default.jpg',
  keywords,
  location = 'Austin, Texas',
  service = 'business infrastructure',
  pageType = 'website',
  isServicePage = false,
  isLocationPage = false,
  serviceName,
  cityName,
} = Astro.props;

const siteUrl = Astro.site?.href ?? 'https://thekpsgroup.com';
const currentUrl = canonical || new URL(Astro.url.pathname, siteUrl).href;

// Aggressive nationwide SEO keyword strategy
const defaultKeywords = [
  // PRIMARY BRAND DOMINANCE - Beat Birmingham KPS Group
  'The KPS Group LLC',
  'The KPS Group Austin Texas',
  'The KPS Group business infrastructure',
  'KPS Group back office solutions',
  'KPS Group QuickBooks experts',
  'KPS Group payroll services',
  'Karson Lawrence KPS Group',
  'Modern business infrastructure KPS',
  
  // QUICKBOOKS DOMINATION (HIGH VALUE TARGET)
  'QuickBooks consulting nationwide',
  'QuickBooks cleanup services',
  'QuickBooks migration experts',
  'QuickBooks job costing contractors',
  'QuickBooks multi-entity consolidation',
  'QuickBooks coaching training',
  'QuickBooks workflow automation',
  'QuickBooks ProAdvisor certified',
  'QuickBooks disaster recovery',
  'QuickBooks for HVAC contractors',
  'QuickBooks for plumbing companies',
  'QuickBooks for electrical contractors',
  'QuickBooks for construction companies',
  
  // PAYROLL PEO SERVICES (MONTHLY REVENUE DRIVER)
  'PEO services small business',
  'payroll processing nationwide',
  'HR compliance consulting',
  'employee benefits administration',
  'workers compensation management',
  'payroll tax compliance',
  'Modern Pay payroll services',
  'PEO for contractors',
  'payroll for trades businesses',
  'HR outsourcing small business',
  
  // ENHANCED WORKFORCE LEADERSHIP SERVICES (PRIMARY FOCUS)
  'workforce leadership development',
  'organizational psychology consulting',
  'positive reinforcement systems',
  'next generation workforce management',
  'contractor website development',
  'trades business branding',
  'professional service websites',
  'local business web design',
  
  // HIGH-VALUE CONSULTING (LONG TERM PLAY)
  'business operations consulting',
  'SOP development consulting',
  'business process optimization',
  'operational excellence consulting',
  'business systems integration',
  'ERP implementation consulting',
  'CRM setup and optimization',
  'business automation consulting',
  'scalability consulting services',
  'Modern Consulting business advisory',
  
  // TEXAS MARKET DOMINATION
  'business services Austin Texas',
  'back office solutions Dallas TX',
  'QuickBooks consulting Houston TX',
  'payroll services San Antonio TX',
  'business consulting Fort Worth TX',
  'operational consulting Texas',
  'business infrastructure Austin',
  'Modern infrastructure Texas',
  
  // NATIONWIDE SERVICE AREAS
  'business consulting United States',
  'QuickBooks support nationwide',
  'payroll services all 50 states',
  'remote business consulting',
  'virtual back office solutions',
  'nationwide business infrastructure',
  
  // INDUSTRY-SPECIFIC LONG TAILS
  'HVAC business consulting',
  'plumbing company back office',
  'electrical contractor QuickBooks',
  'construction company payroll',
  'home services business systems',
  'professional services automation',
  'trades business infrastructure',
  'service business scaling',
  'contractor business consulting',
  
  // COMPETITIVE KEYWORDS
  'best QuickBooks consultants',
  'top business consultants Austin',
  'premier back office solutions',
  'leading business infrastructure',
  'trusted payroll providers',
  'expert business advisors',
  'proven business systems',
  'enterprise-grade solutions SMB'
];

const metaKeywords = keywords ? keywords.join(', ') : defaultKeywords.join(', ');

// Dynamic title optimization
let optimizedTitle = title;
if (isServicePage && serviceName) {
  optimizedTitle = `${serviceName} | Nationwide Experts | The KPS Group`;
} else if (isLocationPage && cityName) {
  optimizedTitle = `Business Services ${cityName} TX | QuickBooks & Payroll | The KPS Group`;
}

// Enhanced meta description with CTAs
let optimizedDescription = description;
if (isServicePage) {
  optimizedDescription = `Expert ${serviceName || service} nationwide. Call 469-534-3392 for consultation. ${description.substring(0, 100)}...`;
} else if (isLocationPage) {
  optimizedDescription = `Professional business services in ${cityName || location}. QuickBooks, payroll, branding & consulting. Call 469-534-3392 today.`;
}
